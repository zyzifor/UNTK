FUNCTION procmessVlv
(* ------------------------------------------------------------------------------------------------------------------ *)
(* Name                Data Type      Dimension            Direction  Attribute       Comment                         *)
(* ------------------------------------------------------------------------------------------------------------------ *)
(* procsetsVlv         BOOL                                VarOutput  Write           Результат выполнения функции *)
(* ------------------------------------------------------------------------------------------------------------------ *)
procsetsVlv:= sVlv.iSize;
if sVlv.iSize>sVlv.iMax then procsetsVlv:= -sVlv.iSize; return; end_if;
(* ------------------------------------------------------------------------------------------------------------------ *)
(* Проход по всем параметрам *)
i:= 1;   while i<=sVlv.iSize do
(* ------------------------------------------------------------------------------------------------------------------ *)
(* Отключение резевных объектов *)
if not dVlv[i].nLogicNot then
(* ------------------------------------------------------------------------------------------------------------------ *)
(* Передача в Рабочий объект *)
  iVlv:= dVlv[i];
(* ------------------------------------------------------------------------------------------------------------------ *)
(* Чтение уставок *)
  if iVlv.nSetsRead then
    dSetsData[sVlv.Idn].Data[i, 01] := ANY_TO_REAL (iVlv.tTimer1)/10.0;                   (* Т01 Срабатывание эл. цепей управления <сек.> *)
    dSetsData[sVlv.Idn].Data[i, 02] := ANY_TO_REAL (iVlv.tTimer2)/10.0;                   (* Т02 Сход с конц. выключателей <сек.> *)
    dSetsData[sVlv.Idn].Data[i, 03] := ANY_TO_REAL (iVlv.tTimer3)/10.0;                   (* Т03 Полное открытие или закрытие <сек.> *)
  end_if;
(* ------------------------------------------------------------------------------------------------------------------ *)
(* Запись уставок *)
  if iVlv.nSetsWrite then
    iVlv.tTimer1                    := ANY_TO_WORD (dSetsData[sVlv.Idn].Data[i, 01]*10.0);(* Т01 Срабатывание эл. цепей управления <сек.> *)
    iVlv.tTimer2                    := ANY_TO_WORD (dSetsData[sVlv.Idn].Data[i, 02]*10.0);(* Т02 Сход с конц. выключателей <сек.> *)
    iVlv.tTimer3                    := ANY_TO_WORD (dSetsData[sVlv.Idn].Data[i, 03]*10.0);(* Т03 Полное открытие или закрытие <сек.> *)
  end_if;
(* ------------------------------------------------------------------------------------------------------------------ *)
(* Передача из Рабочего объекта *)
  dVlv[i]:= iVlv;
(* ------------------------------------------------------------------------------------------------------------------ *)
(* Отключение резевных объектов *)
end_if;
i:= i+1; end_while;
END_FUNCTION
