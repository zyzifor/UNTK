FUNCTION procmessDso
(* ------------------------------------------------------------------------------------------------------------------ *)
(* Name                Data Type      Dimension            Direction  Attribute       Comment                         *)
(* ------------------------------------------------------------------------------------------------------------------ *)
(* procsetsDso         BOOL                                VarOutput  Write           Результат выполнения функции *)
(* ------------------------------------------------------------------------------------------------------------------ *)
procsetsDso:= sDso.iSize;
if sDso.iSize>sDso.iMax then procsetsDso:= -sDso.iSize; return; end_if;
(* ------------------------------------------------------------------------------------------------------------------ *)
(* Проход по всем параметрам *)
i:= 1;   while i<=sDso.iSize do
(* ------------------------------------------------------------------------------------------------------------------ *)
(* Отключение резевных объектов *)
if not dDso[i].nLogicNot then
(* ------------------------------------------------------------------------------------------------------------------ *)
(* Передача в Рабочий объект *)
  iDso:= dDso[i];
(* ------------------------------------------------------------------------------------------------------------------ *)
(* Чтение уставок *)
  if iDso.nSetsRead then
    dSetsData[sDso.Idn].Data[i, 01] := ANY_TO_REAL (iDso.tTimer1)/10.0;                   (* Т01 Импульс Включить (Импульс) <сек.> *)
    dSetsData[sDso.Idn].Data[i, 02] := ANY_TO_REAL (iDso.tTimer2)/10.0;                   (* Т02 Импульс Отключить (Мигание) <сек.> *)
  end_if;
(* ------------------------------------------------------------------------------------------------------------------ *)
(* Запись уставок *)
  if iDso.nSetsWrite then
    iDso.tTimer1                    := ANY_TO_WORD (dSetsData[sDso.Idn].Data[i, 01]*10.0);(* Т01 Импульс Включить (Импульс) <сек.> *)
    iDso.tTimer2                    := ANY_TO_WORD (dSetsData[sDso.Idn].Data[i, 02]*10.0);(* Т02 Импульс Отключить (Мигание) <сек.> *)
  end_if;
(* ------------------------------------------------------------------------------------------------------------------ *)
(* Передача из Рабочего объекта *)
  dDso[i]:= iDso;
(* ------------------------------------------------------------------------------------------------------------------ *)
(* Отключение резевных объектов *)
end_if;
i:= i+1; end_while;
END_FUNCTION
