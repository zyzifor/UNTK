FUNCTION controlDioMK541N_In
(* ------------------------------------------------------------------------------------------------------------------ *)
(* Name                Data Type      Dimension            Direction  Attribute       Comment                         *)
(* ------------------------------------------------------------------------------------------------------------------ *)
(* controlDioMK541N    BOOL                                VarOutput  Write           Результат выполнения функции *)
(* iPrgModify          BOOL                                VarInput   Read            Флаг полной перезагрузки программы ПЛК *)
(* iPrgChange          BOOL                                VarInput   Read            Флаг изменения программы ПЛК *)
(* i                   DINT                                Var        Read/Write      Счётчик *)
(* _bRes               BOOL                                Var        Read/Write      Результат функции BOOL *)
(* _dRes               DINT                                Var        Read/Write      Результат функции DINT *)
(* ------------------------------------------------------------------------------------------------------------------ *)
controlDioMK541N_In := true;
(* ------------------------------------------------------------------------------------------------------------------ *)
(* Функция не выполняется при перезагрузке программы ПЛК *)
if iPrgModify then controlDioMK541N_In:= false; return; end_if;
(* ------------------------------------------------------------------------------------------------------------------ *)
(* Проход по всем параметрам *)
i:= 1;   while i<=sDioMK541N.iSize do
(* ------------------------------------------------------------------------------------------------------------------ *)
(* Передача в Рабочий объект *)
  iDioMK541N:= dDioMK541N[i];
(* ------------------------------------------------------------------------------------------------------------------ *)
  (* Обмен данными *)
  case i of
     1: ; (* А1.10             ЩЭО. МК-541-002 (rs485)                            *)
        dDioMK541NData[i].dMBData:= iodA1_10_001_RS485NEW;
        dDioMK541NData[i].xP1Status:= iop1A1_10_001_RS485NEW;

  end_case;
(* ------------------------------------------------------------------------------------------------------------------ *)
(* Передача из Рабочего объекта *)
  dDioMK541N[i]:= iDioMK541N;
(* ------------------------------------------------------------------------------------------------------------------ *)
(* Отключение резевных объектов *)
i:= i+1; end_while;
END_FUNCTION