FUNCTION controlInit
controlInit:= true;
(* ------------------------------------------------------ *)
(* читаем флаг активации процедуры Инициализации уставок*)
  _dRes       := SafeCopyFromModbusREGsArray(iodMBP_4x, iData, 15, 640, 0, 0, 2); (*Копирование из iodMBP_4x в iData слова расположенного по адресу 16000*)
  _iWord      := iData[0,0];
  cInit       :=  cInit or _iWord.0;
(* -------------------------------------------------------------------------------------- *)
END_FUNCTION